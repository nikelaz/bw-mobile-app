{
  "expo": {
    "name": "Budget Warden",
    "slug": "budget-warden",
    "version": "1.4.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#000000",
    "splash": {
      "backgroundColor": "#ffffff",
      "image": "./assets/splash-icon-light.png",
      "dark": {
        "image": "./assets/splash-icon-dark.png",
        "backgroundColor": "#0D1117"
      },
      "imageWidth": 200
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "com.lazarovco.budgetwarden",
      "icon": {
        "dark": "./assets/ios-dark.png",
        "light": "./assets/ios-light.png",
        "tinted": "./assets/ios-tinted.png"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "monochromeImage": "./assets/adaptive-icon-monochrome.png",
        "backgroundColor": "#ffffff"
      },
      "userInterfaceStyle": "automatic",
      "package": "com.lazarovco.budgetwarden"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow Budget Warden to use Face ID."
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["node_modules/@ant-design/icons-react-native/fonts/antoutline.ttf"]
        }
      ]
    ],
    "newArchEnabled": true,
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9db2f143-7403-4bbe-9df9-bb990d546fee"
      }
    }
  }
}
